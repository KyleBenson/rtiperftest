<?xml version="1.0" encoding="UTF-8"?>

<permissions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="dds_security_permissions.xsd">

    <grant name="ParticipantA">
        <subject_name>/C=US/ST=CA/O=Real Time Innovations/CN=perftest/emailAddress=pub@rti.com</subject_name>
        <validity>
            <!-- Format is YYYYMMDDHH in GMT -->
            <not_before>2013060113</not_before>
            <not_after>2023060113</not_after>
        </validity>
        <allow_rule>
            <domains>
              <id_range>
                <min>0</min>
              </id_range>
            </domains>
            <publish>
                <topic>Throughput</topic>
            </publish>
            <publish>
                <topic>Announcement</topic>
            </publish>
            <subscribe>
                <topic>Latency</topic>
            </subscribe>
            <subscribe>
                <topic>Announcement</topic>
            </subscribe>
        </allow_rule>
        <default>DENY</default>
    </grant>
</permissions>