#
# A makefile for the i86Linux2.6gcc3.4.3 architecture, which
# corresponds to RedHat Enterprise Linux 4.
#

ARCH         := i86Linux2.6gcc3.4.3
DEBUG_FLAG   := -O4
#DEBUG_FLAG   := -g
DEFINES_ARCH_SPECIFIC := -DRTI_LINUX

cxx_cc       := g++
cxx_cc_flags := -m32

cxx_ld       := g++ 
cxx_ld_flags := -m32 -static-libgcc -ldl -lnsl -lm -L/usr/lib/nptl -lpthread -lrt

include common.mk
