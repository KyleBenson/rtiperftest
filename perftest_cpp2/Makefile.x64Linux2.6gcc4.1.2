#
# A makefile for the x64Linux2.6gcc4.1.2 architecture, which
# corresponds to RedHat Enterprise Linux 5.
#

ARCH         := x64Linux2.6gcc4.1.2
DEBUG_FLAG   := -O4
#DEBUG_FLAG   := -g
DEFINES_ARCH_SPECIFIC := -DRTI_LINUX -DRTI_64BIT

cxx_cc       := g++
cxx_cc_flags := -m64

cxx_ld       := g++ 
cxx_ld_flags := -m64 -static-libgcc -ldl -lnsl -lm -L/usr/lib64/nptl -lpthread -lrt

include common.mk
